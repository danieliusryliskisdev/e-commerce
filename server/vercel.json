{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    },
    { "src": "/login", "dest": "/" },
    { "src": "/login/phone-verification", "dest": "/" },
    { "src": "/topics", "dest": "/" },
    { "src": "/", "dest": "/" }
  ]
}
